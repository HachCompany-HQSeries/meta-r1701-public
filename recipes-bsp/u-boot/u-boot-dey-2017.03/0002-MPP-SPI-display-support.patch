diff --git a/board/digi/r1701/genlogo.py b/board/digi/r1701/genlogo.py
index e69de29bb2..54619adc09 100644
--- a/board/digi/r1701/genlogo.py
+++ b/board/digi/r1701/genlogo.py
@@ -0,0 +1,28 @@
+import sys
+
+def reverse_bits(u16):
+    return int('{:016b}'.format(u16)[::-1], 2)
+
+with open("hachlogo.bytes") as logo_bytes:
+    lines = logo_bytes.readlines()
+
+out_buf = ""
+
+for i in range(len(lines)):
+    rev_addr = reverse_bits((i+1)<<6)
+    #out_buf += str(i) + ", " + str(rev_addr) + ", "
+    out_buf += '0x{:x}, 0x{:x}, '.format(1<<7|rev_addr>>8, rev_addr&0xff)
+    out_buf += lines[i]
+
+out_buf += "0x00,0x00"
+
+print("#if 0")
+print("#This file was generated with the following python script: ")
+with open(sys.argv[0]) as f:
+    print(f.read())
+print("'''")
+print("#endif")
+print("const char HachLogoMPP_SPI_buf[] = {")
+print(out_buf)
+print("};")
+print("//'''")

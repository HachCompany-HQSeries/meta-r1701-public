+#include <common.h>
+#include <command.h>
+#include <dm.h>
+#include <errno.h>
+#include <spi.h>
+
+/*
+ */
+
+extern struct spi_slave *r1701_mpp_dev;
+static char helpmsg[] = "<clear>     - clear the screen";
+
+int do_mip (cmd_tbl_t *cmdtp, int flag, int argc, char * const argv[])
+{
+    if(argc == 1){
+        printf("%s\n", helpmsg);
+        return 1;
+    }
+
+    if(strcmp(argv[1], "clear") == 0){
+        char clear[2] = {1<<5,0};
+
+        printf("clearing screen...\n");
+        spi_xfer(r1701_mpp_dev, 8*2, clear, clear, SPI_XFER_ONCE);
+    }
+
+    return 0;
+}
+
+/***************************************************/
+
+U_BOOT_CMD(
+mip,	5,	1,	do_mip,
+	"MIP display control driver",
+    helpmsg
+);

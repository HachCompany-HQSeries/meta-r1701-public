-	status = "okay";
-};
-
 /* Pin mux configuration */
 &iomuxc {
 	imx6ul-ccimx6ul {
@@ -139,3 +126,18 @@
 		//};
 	};
 };
+#else
+&usbotg1 {
+	status = "okay";
+};
+
+/* USDHC2 (microSD) */
+&usdhc2 {
+	status = "okay";
+};
+
+/* UART5 (Console) */
+&uart5 {
+	status = "okay";
+};
+#endif

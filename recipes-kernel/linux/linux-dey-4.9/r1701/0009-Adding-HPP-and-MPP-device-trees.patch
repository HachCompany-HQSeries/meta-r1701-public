+
+// PMIC - VSNVS --> Internal CPU power supply
+&vdd_snvs_3v3 {
+    regulator-min-microvolt = <1000000>;
+    regulator-max-microvolt = <3000000>;
+    regulator-boot-on;
+    regulator-always-on;

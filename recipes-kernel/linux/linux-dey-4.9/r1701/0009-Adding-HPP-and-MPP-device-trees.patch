+            fsl,pins = <
+                MX6UL_PAD_CSI_DATA05__GPIO4_IO26        0x10b0
+            >;
+        };
+
+        // Ortustech VGA LCD pins
+        pinctrl_lcdif: lcdifdatactrlgrp {
